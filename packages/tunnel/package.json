{
  "name": "ra-https-tunnel",
  "version": "0.0.0",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "concurrently -n ts,sw -c green,cyan  \"tsc --watch\" \"esbuild src/sw.ts --bundle --watch --outfile=lib/sw.build.js\"",
    "build": "tsc && esbuild src/sw.ts --bundle --outfile=lib/sw.build.js",
    "test": "ava --serial"
  },
  "exports": {
    ".": "./lib/index.js",
    "./sw": "./lib/swMiddleware.js",
    "./register": "./lib/registerServiceWorker.js"
  },
  "dependencies": {
    "cbor-x": "^1.6.0",
    "libsodium-wrappers": "^0.7.15",
    "node-mocks-http": "^1.17.2",
    "uint8array-extras": "^1.5.0",
    "ra-https-qvl": "^0.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^24.3.1",
    "@types/ws": "^8.18.1",
    "ava": "^6.4.1",
    "concurrently": "^9.2.1",
    "express": "^5.1.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "nodeArguments": [
      "--import=tsx"
    ],
    "files": [
      "test/**/*.test.ts"
    ]
  }
}
